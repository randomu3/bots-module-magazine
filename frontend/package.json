{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend for Telegram Bot Modules Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:firefox": "cypress run --browser firefox",
    "e2e:edge": "cypress run --browser edge",
    "e2e:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "e2e:tablet": "cypress run --config viewportWidth=768,viewportHeight=1024",
    "e2e:desktop": "cypress run --config viewportWidth=1920,viewportHeight=1080",
    "e2e:cross-browser": "node cypress/scripts/run-cross-browser-tests.js",
    "e2e:critical": "cypress run --spec 'cypress/e2e/critical-paths/**/*.cy.ts'",
    "e2e:auth": "cypress run --spec 'cypress/e2e/auth/**/*.cy.ts'",
    "e2e:dashboard": "cypress run --spec 'cypress/e2e/dashboard/**/*.cy.ts'",
    "e2e:admin": "cypress run --spec 'cypress/e2e/admin/**/*.cy.ts'",
    "e2e:responsive": "cypress run --spec 'cypress/e2e/responsive/**/*.cy.ts'"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.292.0",
    "next": "^14.0.1",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.5",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "cypress": "^14.5.4",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nyc": "^17.1.0",
    "typescript": "^5.2.2"
  }
}
